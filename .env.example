# Supabase Configuration (Required)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Analytics Configuration (Optional)
VITE_VERCEL_ANALYTICS=true

# Error Logging Configuration (Optional - Highly Recommended for Production)
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_SENTRY_ENVIRONMENT=production

# Rate Limiting Configuration (Optional)
VITE_RATE_LIMIT_ENABLED=true
VITE_RATE_LIMIT_MAX_REQUESTS=10
VITE_RATE_LIMIT_WINDOW_MS=60000

# Environment (Automatically set by build process)
NODE_ENV=production
